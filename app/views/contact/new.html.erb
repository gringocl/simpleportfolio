<%= form_for @message, url: contact_path do |m|  %>
  <%= m.text_field :name, placeholder: "Name"  %></br>
  <%= m.text_field :email, placeholder: "Email"  %></br>
  <%= m.text_field :subject, placeholder: "Subject"  %></br>
  <%= m.text_area :body, placeholder: "Message"  %> </br>
  <%= m.submit 'Contact Me!'  %>
<% end %>
